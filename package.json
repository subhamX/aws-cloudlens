{
  "name": "agent-starter",
  "version": "1.0.0",
  "description": "A lightning fast starter for autonomous AI agent development",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config src/index.ts",
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db": "bun run drizzle-kit generate && bun run drizzle-kit migrate"
  },
  "keywords": [
    "ai",
    "agent",
    "openserv-labs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ngrok/ngrok": "^1.5.1",
    "@openserv-labs/sdk": "^1.2.0",
    "@types/node-telegram-bot-api": "^0.64.9",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.38.2",
    "node-telegram-bot-api": "^0.66.0",
    "pg": "^8.13.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
